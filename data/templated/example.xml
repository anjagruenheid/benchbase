<?xml version="1.0" encoding="UTF-8"?>
<templates>
   <template name="GetOrder">
      <query><![CDATA[SELECT NO_O_ID FROM new_order WHERE NO_D_ID = ? OR NO_D_ID = ? ORDER BY NO_O_ID ASC]]></query>
      <parameter_types>
         <parameter_type>INTEGER</parameter_type>
         <parameter_type>INTEGER</parameter_type>
      </parameter_types>
      <parameter_values>
         <parameter_value>3</parameter_value>
         <parameter_value>4</parameter_value>
      </parameter_values>
       <parameter_values>
         <parameter_value>5</parameter_value>
         <parameter_value>6</parameter_value>
      </parameter_values>
   </template>
   <template>
      <name>GetCust</name>
      <query><![CDATA[SELECT C_DISCOUNT, C_LAST, C_CREDIT FROM customer WHERE C_W_ID = ?]]></query>
      <parameter_types>
         <parameter_type>INTEGER</parameter_type>
      </parameter_types>
      <parameter_values>
         <parameter_value>8</parameter_value>
      </parameter_values>
   </template>
   <template>
      <name>GetWarehouse</name>
      <query><![CDATA[SELECT * FROM warehouse WHERE W_STREET_1 LIKE ?]]></query>
      <parameter_types>
         <parameter_type>VARCHAR</parameter_type>
      </parameter_types>
      <parameter_values>
         <parameter_value>Str</parameter_value>
      </parameter_values>
   </template>
</templates>